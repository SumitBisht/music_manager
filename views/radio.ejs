     <tr>
       <td colspan=2 align="center"><button name="GetMetaData" onClick="getMetaData()">Start getting song Metadata from URL</button></td>
     </tr>
    </table>
  </div>
  <div id="metadataDisplayer">
    <h2 id="resultingMetaData"></h2>
  </div>
</div>
<%include footer%>