<%include header%>
<script type="text/javascript" src="javascripts/UrlMetadataResolver.js"></script>
<script type="text/javascript" src="javascripts/jquery/jquery-1.7.2.min.js"></script>
<p><%=title%></p>

<div id="streamingurlselector">
  <div id="streamSelector">
    <table>
     <tr>
       <td>Stream URL</td>
       <td><input type="text" name="URL" id="url" default="http://streaming207.radionomy.com:80/MUSICK4U" />
